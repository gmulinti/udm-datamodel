{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"id": "http://openplatform.lumesse.com/schemas/isic",
	"title": "Industry Code",
	"description": "United Nations ISIC codes version 4.0",
	"type": "object",
	"javaType": "com.openplatform.udm.common.IndustryCode",
	"javaInterfaces": ["java.io.Serializable"],
	"required": ["code"],
	"properties": {
		"name": {
			"description": "The textual name of the code. NB Display only and is derived from 'Code' value.",
			"type": "string"
		},
		"code": {
			"description": "The short code to represent the code name.",
			"type": "string",
			"pattern": "^[0-9]{1,4}"
		}
	},
	"additionalProperties": false
	
}